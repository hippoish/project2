<h1><%= @carpool.name %></h1>

<ul>
  <% @carpool.users.each do |user| %>
    <li><%= user.f_name %> <%= user.l_name %></li>
  <% end %>

</ul>

<%= form_for [@neighborhood, @carpool], :url => join_carpool_path, :html => { :method => :patch } do |f|%>
  <%= f.submit 'join this carpool!' %>
<% end %>
